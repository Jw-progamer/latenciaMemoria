<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.TelaSimuladorController">
    <children>
        <HBox prefHeight="172.0" prefWidth="600.0" spacing="15.0">
            <children>
                <TableView fx:id="tabela" editable="false" prefHeight="200.0" prefWidth="269.0">
                    <columns>
                        <TableColumn fx:id="cnome" editable="true" onEditCommit="#addProcesso" prefWidth="75.0" text="Nome">
                            <cellValueFactory>
                                <javafx.scene.control.cell.PropertyValueFactory property="nome" /> 
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="cchegada" editable="true" onEditCommit="#addProcesso" prefWidth="75.0" text="chegada">
                            <cellValueFactory>
                                <javafx.scene.control.cell.PropertyValueFactory property="tempoChegada" /> 
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="cprocesso" editable="true" onEditCommit="#addProcesso" prefWidth="75.0" text="processo">
                            <cellValueFactory>
                                <javafx.scene.control.cell.PropertyValueFactory property="tempoExecucao" /> 
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableView>
                <VBox prefHeight="138.0" prefWidth="101.0" spacing="10.0">
               
                    <children>
                        <RadioButton fx:id="minuto" mnemonicParsing="false" text="minutos" />
                        <RadioButton fx:id="segundo" contentDisplay="TOP" mnemonicParsing="false" selected="true" text="segundos" />
                    </children>
                    <opaqueInsets>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </opaqueInsets>
                </VBox>
            <VBox prefHeight="138.0" prefWidth="210.0" spacing="10.0">
               <children>
                  <Button fx:id="botao" mnemonicParsing="false" onAction="#addProcesso" text="Adicionar novo processo" />
                  <Button fx:id="inicia" mnemonicParsing="false" onAction="#callSimulacao" prefHeight="26.0" prefWidth="178.0" text="Iniciar" />
                  <HBox>
                     <children>
                        <Label prefHeight="16.0" prefWidth="147.0" text="Ociosidade:" />
                        <TextField fx:id="ociosidade" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
